{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "airbnb",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:security/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12
    },
    "plugins": [
      "react",
      "prettier",
      "security"
    ],
    "rules": {
      "no-console": "error",
      "consistent-return": "off",
      "func-names": "off",
      "security/detect-object-injection": "off"
    }
  }
  